generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Games {
  id         String     @id @default(uuid())
  createdAt  DateTime   @default(now())
  clientId   String     @unique
  endTime    String?
  foundArray Boolean[]  @default([false, false, false])
}

model TopGames {
  id        String   @id @default(uuid())
  score     String   
  username  String   
  place     String   
}
